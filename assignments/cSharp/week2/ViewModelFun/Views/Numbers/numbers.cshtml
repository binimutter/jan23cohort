@{
    ViewData["Title"] = "Numbers";
}
@model int[];

<div class="text-center">
    <h1>Here are some numbers!</h1>
    @{
        foreach(int num in Model) {
            <h3>@num</h3>
        }
        @* for(int i = 0; i <Model.Length; i++) {
            <h5>@Model[i]</h5>
        } *@
    }
</div>